<%- if controller_name != 'sessions' %>
	<p>Already have an account? Then <br/><%= link_to "Sign In", new_session_path(resource_name) ,style: "color: #FFA500;" %><br /> </p>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
	<p>Don't have an account? Then <br/> <%= link_to "Sign up", new_registration_path(resource_name) ,style: "color: #FFA500;"%></p>
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Forgot your password?", new_password_path(resource_name) ,style: "color: #FFA500;"%><br />
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name) ,style: "color: #FFA500;"%><br />
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name) ,style: "color: #FFA500;"%><br />
<% end -%>

